{
  "name": "nextplayerup",
  "version": "1.0.0",
  "description": "This application is created with the Next.js framework. I will be using Flexbox to style.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "watch:sass": "node-sass styles/sass/main.scss styles/css/styles.css -w",
    "dev": "next dev",
    "build": "next build",
    "start": "npm-run-all --parallel dev watch:sass",
    "export": "next export",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhysbrame/nextPlayerUp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rhysbrame/nextPlayerUp/issues"
  },
  "homepage": "https://github.com/rhysbrame/nextPlayerUp#readme",
  "dependencies": {
    "next": "^11.1.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "cypress": "^8.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1"
  }
}